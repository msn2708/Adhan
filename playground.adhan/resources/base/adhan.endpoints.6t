endpoints {
  action-endpoints {
    action-endpoint (GetPrayerTimes) {
      accepted-inputs (date, method, school,latitudeAdjustmentMethod,prayerName,resolvedAddress,$vivContext)
      local-endpoint ("GetPrayerTimes.js")
    }
    action-endpoint (ShowMethods) {
      accepted-inputs (method)
      local-endpoint ("ShowMethods.js")
    }
    action-endpoint (PromptUserForLocation) {
      accepted-inputs (userPromptedLocation)
      local-endpoint ("getUserPromptedLocation.js")
    }

  }
}
